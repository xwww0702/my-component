  "main": "index.js",
  "scripts": {
    "dev": " pnpm --filter @my-component/play dev",
    "docs:dev": "pnpm --filter @my-component/docs dev",
    "docs:build": "pnpm --filter @my-component/docs build",
    "test": "run-p test-utils test-comp",
    "story": "pnpm --filter @my-component/play storybook",
    "test-utils": "cross-env NODE_ENV=test pnpm --filter @my-component/utils test ",
    "test-comp": "cross-env NODE_ENV=test pnpm --filter @my-component/components test ",
    "build": "run-s build-hooks build-comp",
    "build-hooks": "cross-env NODE_ENV=production pnpm --filter @my-component/hooks build",
    "build-comp": "cross-env NODE_ENV=production pnpm --filter my-component build",
    "build:dev": "cross-env NODE_ENV=development pnpm --filter my-component build:watch",
    "changed": "dotenv lerna version --conventional-commits --create-release github --yes",
    "changed:patch": "dotenv lerna version patch --conventional-commits --create-release github --yes",
    "changed:major": "dotenv lerna version major --conventional-commits --create-release github --yes",
    "release": "lerna publish from-package --yes"
  },
